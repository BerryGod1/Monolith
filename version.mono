                        elseif ping > 110 and ping < 140 then 
                            Lynx.Aimlock.Pred = 0.15934
                        elseif ping < 105 and ping > 90 then
                            Lynx.Aimlock.Pred = 0.138
                        elseif ping < 90 and ping > 80 then
