local Namecall; Namecall = hookmetamethod(game, "__namecall", function(self, ...)
    local args = { ... }
    local method = getnamecallmethod()

    if method == "FireServer" then
        if args[1] == "UpdateMousePos" or args[1] == "MousePos" or args[1] == "Mouse" or args[1] == "MOUSE" or args[1] == "UpdateMousePosI" then
            if Lynx.Aimlock.Enabled and Lynx.Target then
                if Lynx.Aimlock.Resolver then
                    args[2] = Lynx.Target.Character[Lynx.Aimlock.Part].Position
                    return Namecall(self, unpack(args))
                elseif Lynx.Aimlock.Prediction then
                    args[2] = Lynx.Target.Character[Lynx.Aimlock.Part].Position + (Lynx.Target.Character[Lynx.Aimlock.Part].Velocity * Lynx.Aimlock.Pred)
                    return Namecall(self, unpack(args))
                elseif Lynx.Aimlock.AirShot and Lynx.Target.Character:FindFirstChild("Humanoid") and Lynx.Target.Character.Humanoid.FloorMaterial == Enum.Material.Air then
                    args[2] = Lynx.Target.Character[Lynx.Aimlock.AirPart].Position + (Lynx.Target.Character[Lynx.Aimlock.AirPart].Velocity * Lynx.Aimlock.Pred)
                    return Namecall(self, unpack(args))
                else
                    args[2] = Lynx.Target.Character[Lynx.Aimlock.Part].Position
                    return Namecall(self, unpack(args))
                end
            end
            if Lynx.SilentAim.Enabled and Lynx.SilentAim.Target then
                if Lynx.SilentAim.Resolver then
                    args[2] = Lynx.SilentAim.Target.Character[Lynx.SilentAim.Part].Position
                    return Namecall(self, unpack(args))
                elseif Lynx.SilentAim.Prediction then
                    args[2] = Lynx.SilentAim.Target.Character[Lynx.SilentAim.Part].Position + (Lynx.SilentAim.Target.Character[Lynx.SilentAim.Part].Velocity * Lynx.SilentAim.Pred)
                    return Namecall(self, unpack(args))
                elseif Lynx.SilentAim.AirShot and Lynx.SilentAim.Target.Character:FindFirstChild("Humanoid") and Lynx.SilentAim.Target.Character.Humanoid.FloorMaterial == Enum.Material.Air then
                    args[2] = Lynx.SilentAim.Target.Character[Lynx.SilentAim.AirPart].Position + (Lynx.SilentAim.Target.Character[Lynx.SilentAim.AirPart].Velocity * Lynx.SilentAim.Pred)
                    return Namecall(self, unpack(args))
                else
                    args[2] = Lynx.SilentAim.Target.Character[Lynx.SilentAim.Part].Position
                    return Namecall(self, unpack(args))
                end
            end
        end
    end

    return Namecall(self, ...)
end)



