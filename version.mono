            if TeleportTarget and TeleportTarget.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                if Lynx.PTele.Type == "Tween" then
                    TweenService:Create(LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(Lynx.PTele.Speed, Enum.EasingStyle.Linear), {CFrame = TeleportTarget.Character.HumanoidRootPart.CFrame}):Play()
                elseif Lynx.PTele.Type == "CFrame" then
